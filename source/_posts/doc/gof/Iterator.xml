<?xml version="1.0" encoding="UTF-8"?>
<pattern:specification xmlns="http://www.ibm.com/software/rational/designpattern" xmlns:argument="http://www.ibm.com/software/rational/designpattern/argument" xmlns:attribute="http://www.ibm.com/software/rational/designpattern/attribute" xmlns:dependency="http://www.ibm.com/software/rational/designpattern/dependency" xmlns:operation="http://www.ibm.com/software/rational/designpattern/operation" xmlns:participant="http://www.ibm.com/software/rational/designpattern/participant" xmlns:pattern="http://www.ibm.com/software/rational/designpattern/pattern" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ibm.com/software/rational/designpattern ..\schemas\designpattern.xsd">
	<pattern:name>Iterator</pattern:name>
	<pattern:description>iterator.html</pattern:description>
	<pattern:assignedLanguage>Analysis</pattern:assignedLanguage>
	<pattern:participants>
		<!-- Client -->
		<pattern:participant>
			<participant:name>Client</participant:name>
			<participant:required>false</participant:required>
			<participant:exportControl>public</participant:exportControl>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:isAnyType>true</participant:isAnyType>
				<!-- attributes -->
				<participant:attributes>
					<participant:attribute>
						<attribute:name>the%Aggregate%</attribute:name>
						<attribute:type>%Aggregate%</attribute:type>
						<attribute:exportControl>private</attribute:exportControl>
						<attribute:cardinality>1</attribute:cardinality>
					</participant:attribute>
					<participant:attribute>
						<attribute:name>the%Iterator%</attribute:name>
						<attribute:type>%Iterator%</attribute:type>
						<attribute:exportControl>private</attribute:exportControl>
						<attribute:cardinality>1</attribute:cardinality>
					</participant:attribute>
				</participant:attributes>
		</pattern:participant>
		<!--Iterator -->
		<pattern:participant>
			<participant:name>Iterator</participant:name>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:required>false</participant:required>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>false</participant:isAbstract>
			<participant:isInterface>true</participant:isInterface>
			<!--
			<participant:documentation><![CDATA[	
Iterator. Defines an interface for accessing and traversing elements. 
]]></participant:documentation>
-->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>First</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<!--
					<operation:documentation><![CDATA[
Move to the top of the list.
]]></operation:documentation>
-->
				</participant:operation>
				<participant:operation>
					<operation:name>Next</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<!--
					<operation:documentation><![CDATA[
Move next through the list.
]]></operation:documentation>
-->
				</participant:operation>
				<participant:operation>
					<operation:name>IsDone</operation:name>
					<operation:returnType>boolean</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<!--
					<operation:documentation><![CDATA[
Find out if there are more elements in the list.
]]></operation:documentation>
-->
				</participant:operation>
				<participant:operation>
					<operation:name>CurrentItem</operation:name>
					<operation:returnType>%ConcreteAggregate%</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<!--
					<operation:documentation><![CDATA[
Find the current item in the list.
]]></operation:documentation>
-->
				</participant:operation>
			</participant:operations>
		</pattern:participant>
		<!--Aggregate-->
		<pattern:participant>
			<participant:name>Aggregate</participant:name>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:required>true</participant:required>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>false</participant:isAbstract>
			<participant:isInterface>true</participant:isInterface>
			<!--
			<participant:documentation><![CDATA[
Aggregate. Defines an interface for creating an Iterator object. 
]]></participant:documentation>
-->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>CreateIterator</operation:name>
					<operation:returnType>%Iterator%</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<!--
					<operation:documentation><![CDATA[
Creates an iterator object
]]></operation:documentation>
-->
					<!-- arguments -->
				</participant:operation>
			</participant:operations>
       	</pattern:participant>
		<pattern:simpleParticipantCorrelation>
			<!-- Concrete Iterators -->
			<!-- We will describe only a generic one.  All the rest will be similar -->
			<pattern:participant>
				<participant:name>ConcreteIterator</participant:name>
				<participant:multiplicity>n</participant:multiplicity>
				<participant:required>true</participant:required>
				<participant:exportControl>public</participant:exportControl>
				<participant:isAbstract>false</participant:isAbstract>
				<participant:isInterface>false</participant:isInterface>
				<!--
				<participant:documentation><![CDATA[
Concrete Iterator. Implements the Iterator interface. Keeps track of the current position in the traversal of the aggregate.
]]></participant:documentation>
-->
				<!-- attributes -->
				<participant:attributes>
					<participant:attribute>
						<attribute:name>ref%ConcreteAggregate%</attribute:name>
						<attribute:type>%ConcreteAggregate%</attribute:type>
						<attribute:exportControl>private</attribute:exportControl>
						<attribute:cardinality>1</attribute:cardinality>
						<!--
						<attribute:documentation><![CDATA[
reference to ConcreteAggregate
]]></attribute:documentation>
-->
					</participant:attribute>
				</participant:attributes>
				<!-- operations -->
				<participant:operations>
					<participant:operation>
						<operation:name>First</operation:name>
						<operation:returnType>void</operation:returnType>
						<operation:exportControl>public</operation:exportControl>
						<!-- arguments -->
					</participant:operation>
					<participant:operation>
						<operation:name>Next</operation:name>
						<operation:returnType>void</operation:returnType>
						<operation:exportControl>public</operation:exportControl>
						<!-- arguments -->
					</participant:operation>
					<participant:operation>
						<operation:name>IsDone</operation:name>
						<operation:returnType>boolean</operation:returnType>
						<operation:exportControl>public</operation:exportControl>
						<!-- arguments -->
					</participant:operation>
					<participant:operation>
						<operation:name>CurrentItem</operation:name>
						<operation:returnType>%ConcreteAggregate%</operation:returnType>
						<operation:exportControl>public</operation:exportControl>
						<!-- arguments -->
					</participant:operation>
				</participant:operations>
				<!-- relationships -->
				<!-- Implements the classes: -->
				<participant:interfacesImplemented>
					<participant:implements>%Iterator%</participant:implements>
				</participant:interfacesImplemented>
			</pattern:participant>
			<!--Concrete Aggregates -->
			<!--We will describe only a generic one.  All the rest will be similar-->
			<pattern:participant>
				<participant:name>ConcreteAggregate</participant:name>
				<participant:multiplicity>n</participant:multiplicity>
				<participant:required>true</participant:required>
				<participant:exportControl>public</participant:exportControl>
				<participant:isAbstract>false</participant:isAbstract>
				<participant:isInterface>false</participant:isInterface>
				<!--
				<participant:documentation><![CDATA[
ConcreteAggregate. Implements the Iterator creation interface to return an instance of the proper ConcreteIterator. 
]]></participant:documentation>
-->
				<!-- relationships -->
				<!-- Implements the interfaces: -->
				<participant:interfacesImplemented>
					<participant:implements>%Aggregate%</participant:implements>
				</participant:interfacesImplemented>
				<!-- dependency (creates) relationship -->
				<participant:dependencies>
					<participant:dependency>
						<dependency:dependentOn>%ConcreteIterator%</dependency:dependentOn>
						<dependency:type>creates</dependency:type>
					</participant:dependency>
				</participant:dependencies>
				<!-- attributes -->
				<!-- operations -->
				<participant:operations>
					<participant:operation>
						<operation:name>CreateIterator</operation:name>
						<operation:returnType>%Iterator%</operation:returnType>
						<operation:exportControl>public</operation:exportControl>
						<!--
						<operation:documentation><![CDATA[
Sends itself to the ConcreteIterator. 
]]></operation:documentation>
-->
						<!-- arguments -->
					</participant:operation>
				</participant:operations>
			</pattern:participant>
		</pattern:simpleParticipantCorrelation>
	</pattern:participants>
</pattern:specification>
