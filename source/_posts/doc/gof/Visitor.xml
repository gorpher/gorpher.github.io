<?xml version="1.0" encoding="UTF-8"?>
<pattern:specification xmlns="http://www.ibm.com/software/rational/designpattern" xmlns:argument="http://www.ibm.com/software/rational/designpattern/argument" xmlns:attribute="http://www.ibm.com/software/rational/designpattern/attribute" xmlns:dependency="http://www.ibm.com/software/rational/designpattern/dependency" xmlns:operation="http://www.ibm.com/software/rational/designpattern/operation" xmlns:participant="http://www.ibm.com/software/rational/designpattern/participant" xmlns:pattern="http://www.ibm.com/software/rational/designpattern/pattern" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.ibm.com/software/rational/designpattern ..\schemas\designpattern.xsd">
	<pattern:name>Visitor</pattern:name>
	<pattern:description>visitor.html</pattern:description>
	<pattern:assignedLanguage>Analysis</pattern:assignedLanguage>
	<pattern:participants>
		<!-- Client -->
		<pattern:participant>
			<participant:name>Client</participant:name>
			<participant:required>false</participant:required>
			<participant:exportControl>public</participant:exportControl>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:isAnyType>true</participant:isAnyType>
			<!-- dependency relationship -->
			<participant:dependencies>
				<participant:dependency>
					<dependency:dependentOn>%Visitor%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
				<participant:dependency>
					<dependency:dependentOn>%ObjectStructure%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
			</participant:dependencies>
		</pattern:participant>
		<!-- Visitor -->
		<pattern:participant>
			<participant:name>Visitor</participant:name>
			<participant:required>true</participant:required>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>true</participant:isAbstract>
			<participant:isInterface>false</participant:isInterface>
<!--
			<participant:documentation><![CDATA[
Visitor abstract class.  declares a Visit operation for each class of ConcreteElement in the object structure.
]]></participant:documentation>
-->
			<!-- attributes -->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>visit</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<operation:isAbstract>true</operation:isAbstract>
<!--
					<operation:documentation><![CDATA[
one visit operation for each Concrete Element
]]></operation:documentation>
-->
					<!-- arguments -->
					<operation:arguments>
						<operation:argument>
							<argument:name>the%ConcreteElement%</argument:name>
							<argument:type>%ConcreteElement%</argument:type>
						</operation:argument>
					</operation:arguments>
				</participant:operation>
			</participant:operations>
			<!-- relationships -->
			<!-- dependency (uses) relationship -->
			<participant:dependencies>
				<participant:dependency>
					<dependency:dependentOn>%ConcreteElement%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
			</participant:dependencies>
		</pattern:participant>
		<!-- Element-->
		<pattern:participant>
			<participant:name>Element</participant:name>
			<participant:required>true</participant:required>
			<participant:multiplicity>1</participant:multiplicity>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>true</participant:isAbstract>
			<participant:isInterface>false</participant:isInterface>
<!--
			<participant:documentation><![CDATA[
Element abstract class.  Defines an Accept operation that takes a Visitor as an argument.
]]></participant:documentation>
-->
			<!-- attributes -->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>accept</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<operation:isAbstract>true</operation:isAbstract>
<!--
					<operation:documentation><![CDATA[
abstract accept.  Will be implemented in the concrete elements
]]></operation:documentation>
-->
					<!-- arguments -->
					<operation:arguments>
						<operation:argument>
							<argument:name>visitor</argument:name>
							<argument:type>%Visitor%</argument:type>
						</operation:argument>
					</operation:arguments>
				</participant:operation>
			</participant:operations>
			<!-- relationships -->
			<!-- dependency (uses) relationship -->
			<participant:dependencies>
				<participant:dependency>
					<dependency:dependentOn>%Visitor%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
			</participant:dependencies>
		</pattern:participant>
		<!-- ConcreteVisitor -->
		<!-- We will describe only a generic one.  All the rest will be similar -->
		<pattern:participant>
			<participant:name>ConcreteVisitor</participant:name>
			<participant:required>false</participant:required>
			<participant:multiplicity>n</participant:multiplicity>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>false</participant:isAbstract>
			<participant:isInterface>false</participant:isInterface>
<!--
			<participant:documentation><![CDATA[
Concrete visitor class. Implements polymorphic visit() methods for each of the Concrete elements
]]></participant:documentation>
-->
			<!-- attributes -->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>visit</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<operation:isAbstract>false</operation:isAbstract>
					<!-- arguments -->
					<operation:arguments>
						<operation:argument>
							<argument:name>the%ConcreteElement%</argument:name>
							<argument:type>%ConcreteElement%</argument:type>
						</operation:argument>
					</operation:arguments>
				</participant:operation>
			</participant:operations>
			<!-- relationships -->
			<!-- Extends classes: -->
			<participant:extends>%Visitor%</participant:extends>
			<!-- relationships -->
			<!-- dependency (uses) relationship -->
			<participant:dependencies>
				<participant:dependency>
					<dependency:dependentOn>%ConcreteElement%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
			</participant:dependencies>
		</pattern:participant>
		<!-- ConcreteElement -->
		<!-- We will describe only a generic one.  All the rest will be similar -->
		<pattern:participant>
			<participant:name>ConcreteElement</participant:name>
			<participant:required>true</participant:required>
			<participant:multiplicity>n</participant:multiplicity>
			<participant:exportControl>public</participant:exportControl>
			<participant:isAbstract>false</participant:isAbstract>
			<participant:isInterface>false</participant:isInterface>
<!--
			<participant:documentation><![CDATA[
Concrete element class. Implements an accept method() for the Visitor to visit it
]]></participant:documentation>
-->
			<!-- attributes -->
			<!-- operations -->
			<participant:operations>
				<participant:operation>
					<operation:name>accept</operation:name>
					<operation:returnType>void</operation:returnType>
					<operation:exportControl>public</operation:exportControl>
					<operation:isAbstract>false</operation:isAbstract>
					<!-- arguments -->
					<operation:arguments>
						<operation:argument>
							<argument:name>visitor</argument:name>
							<argument:type>%Visitor%</argument:type>
						</operation:argument>
					</operation:arguments>
				</participant:operation>
			</participant:operations>
			<!-- relationships -->
			<!-- Extends classes: -->
			<participant:extends>%Element%</participant:extends>
			<!-- relationships -->
			<!-- dependency (uses) relationship -->
			<participant:dependencies>
				<participant:dependency>
					<dependency:dependentOn>%Visitor%</dependency:dependentOn>
					<dependency:type>uses</dependency:type>
				</participant:dependency>
			</participant:dependencies>
		</pattern:participant>
	</pattern:participants>
</pattern:specification>
